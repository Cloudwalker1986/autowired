{
    "name": "autowired/autowired",
    "description": "With the new Autowired component you have now the possibility to easily load all your dependencies during construction of your main object. ",
    "type": "library",
    "keywords": ["autwired", "autoloading", "dependency injection", "DI", "PHP 8"],
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dennis Munchausen",
            "email": "munchausen.dennis@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "8.0.*"
    },
    "autoload": {
        "psr-4": {
            "Autowired\\": "src/Autowired",
            "AutowiredTest\\": "tests/AutowiredTest"
        }
    },
    "scripts": {
        "all-tests": [
            "vendor/bin/phpunit tests/"
        ]
    }
}
